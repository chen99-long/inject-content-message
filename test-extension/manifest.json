{
  "manifest_version": 3,
  "name": "Event Messaging Test Extension",
  "version": "1.0",
  "description": "测试 inject-content-message 库在真实插件环境中的表现",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js", "index.umd.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
